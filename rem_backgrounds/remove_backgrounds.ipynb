   "source": [
    "#python3 -m pip install rembg\n",
    "from rembg import remove as rem\n",
    "from PIL import Image"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Remove_bg(img):\n",
    "    output = \"removed_bg.png\" \n",
    "    input = Image.open(img)\n",
    "    output_img = rem(input)\n",
    "    output_img.save(output)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
